A: struct {
  let name

  'create: (#new()) => {
    self.name: "A"
  }
}

B: struct {
  let name

  'create: (#new()) => {
    self.name: "B"
  }

  'hello: (#self()) => {
    #println("Hello, I am an instance of " + (self.name) + "!")
  }
}

A -> B: {
  to.name: from.name
}

let a: A.'create()
let b: B.'create()

(a -> B).'hello()
b.'hello()
